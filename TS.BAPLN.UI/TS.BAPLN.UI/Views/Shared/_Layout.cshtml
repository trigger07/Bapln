    <![endif]-->
    <!-- ================== END BASE JS ================== -->
    @RenderSection("scripts", required: false)
    <script>
        $(document).ready(function () {
            App.init();
            Dashboard.init();
            //FormPlugins.init();
            TableManageTableSelect.init();
        });
    </script>
    <style>
        .pace-activity {
            display: none !important;
            visibility: hidden !important;
        }
    </style>
</body>
</html>
