<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    @section scripts
    {
        <script src="~/plugins/jquery-jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
        <script src="~/plugins/jquery-jvectormap/jquery-jvectormap-world-mill-en.js"></script>
        <link href="~/plugins/jquery-jvectormap/jquery-jvectormap-1.2.2.css" rel="stylesheet" />        
    }
    <link href="~/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" />

    <!-- ================== BEGIN BASE CSS STYLE ================== -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet">
    <link href="~/plugins/jquery-ui/css/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/plugins/jquery-ui/themes/base/minified/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/animate.min.css" rel="stylesheet" />
    <link href="~/css/style.min.css" rel="stylesheet" />
    <link href="~/css/style-responsive.min.css" rel="stylesheet" />
    <link href="~/css/theme/default.css" rel="stylesheet" id="theme" />
    <!-- ================== END BASE CSS STYLE ================== -->
    <link href="~/plugins/bootstrap3-editable/css/bootstrap-editable.css" rel="stylesheet" />    
    <link href="~/plugins/bootstrap-datepicker/css/datepicker.css" rel="stylesheet" />
    <link href="~/plugins/bootstrap-datepicker/css/datepicker3.css" rel="stylesheet" />
    <link href="~/plugins/gritter/css/jquery.gritter.css" rel="stylesheet" />
    <link href="~/plugins/DataTables/media/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="~/plugins/DataTables/extensions/Select/css/select.bootstrap.min.css" rel="stylesheet" />
    <link href="~/plugins/DataTables/extensions/Responsive/css/responsive.bootstrap.min.css" rel="stylesheet" />

    <script src="~/plugins/pace/pace.min.js"></script>

    <script src="~/plugins/jquery/jquery-1.9.1.min.js"></script>
    <script src="~/plugins/jquery/jquery-migrate-1.1.0.min.js"></script>
    <script src="~/plugins/jquery-ui/ui/minified/jquery-ui.min.js"></script>
    <script src="~/plugins/jquery-ui/ui/jquery-ui.min.js"></script>
    <script src="~/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/plugins/jquery-cookie/jquery.cookie.js"></script>

    <script src="~/plugins/gritter/js/jquery.gritter.js"></script>
    <script src="~/plugins/flot/jquery.flot.min.js"></script>
    <script src="~/plugins/flot/jquery.flot.time.min.js"></script>
    <script src="~/plugins/flot/jquery.flot.resize.min.js"></script>
    <script src="~/plugins/flot/jquery.flot.pie.min.js"></script>
    <script src="~/plugins/sparkline/jquery.sparkline.js"></script>
    <script src="~/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script src="~/js/dashboard.min.js"></script>
    <script src="~/plugins/DataTables/media/js/jquery.dataTables.js"></script>
    <script src="~/plugins/DataTables/media/js/dataTables.bootstrap.min.js"></script>
    <script src="~/plugins/DataTables/extensions/Select/js/dataTables.select.min.js"></script>
    <script src="~/plugins/DataTables/extensions/Responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/js/table-manage-select.demo.min.js"></script>
    <script src="~/js/apps.min.js"></script>
    <script src="~/js/Utilidades.js"></script>

    <script src="~/plugins/bootstrap3-editable/js/bootstrap-editable.js"></script>
    <script src="~/plugins/bootstrap3-editable/js/bootstrap-editable.min.js"></script>

    <title>Becas con Amor por los niños</title>
</head>
<body>
    <div id="page-container" class="page-container fade page-without-sidebar page-header-fixed page-with-top-menu">
        <div id="header" class="header navbar navbar-default navbar-fixed-top">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a href="#" class="navbar-brand"><span class="navbar-logo"></span> Becas con Amor por los Niños.</a>
                    <button type="button" class="navbar-toggle" data-click="top-menu-toggled">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        <form class="navbar-form full-width">
                            <div class="form-group">

                            </div>
                        </form>
                    </li>
                    <li class="dropdown navbar-user">
                        <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">
                            <img src="~/img/userGray.jpg" alt="" />
                            <span class="hidden-xs">Usuario Administrador</span> <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu animated fadeInLeft">
                            <li class="arrow"></li>
                            <li><a href="javascript:;">Editar Perfil</a></li>
                            <li><a href="javascript:;">Cambio de Contraseña</a></li>
                            <li class="divider"></li>
                            <li><a href="javascript:;">Salir</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
        <div id="top-menu" class="top-menu">
            <ul class="nav">
                <li class="has-sub">
                    <a href="javascript:;">
                        <b class="caret pull-right"></b>
                        <i class="fa fa-users"></i>
                        <span>Estudiantes</span> 
                    </a>
                    <ul class="sub-menu">
                        <li>@Html.ActionLink("Listado", "Estudiantes", "Estudiante")</li>
                        <li>@Html.ActionLink("Nuevo", "Crear", "Estudiante")</li>
                    </ul>
                </li>
                <li class="has-sub">
                    <a href="javascript:;">
                        <b class="caret pull-right"></b>
                        <i class="fa fa-cogs"></i>
                        <span>Donadores</span>
                    </a>
                    <ul class="sub-menu">
                        <li>@Html.ActionLink("Listado", "Donadores", "Donador")</li>
                        <li>@Html.ActionLink("Nuevo", "Crear", "Donador")</li>
                    </ul>
                </li>
                <li class="has-sub">
                    <a href="javascript:;">
                        <b class="caret pull-right"></b>
                        <i class="fa fa-book"></i>
                        <span>Becas</span>
                    </a>
                    <ul class="sub-menu">
                        <li>@Html.ActionLink("Listado", "Index", "Beca")</li>
                        <li>@Html.ActionLink("Nueva", "Create", "Beca")</li>
                        <li>@Html.ActionLink("Asignar a Estudiante", "Create", "BecaEstudiante")</li>
                    </ul>
                </li>
                <li class="has-sub">
                    <a href="javascript:;">
                        <b class="caret pull-right"></b>
                        <i class="fa fa-institution"></i>
                        <span>Instituciones</span>
                    </a>
                    <ul class="sub-menu">
                        <li>@Html.ActionLink("Listado", "Instituciones", "Institucion")</li>
                        <li>@Html.ActionLink("Nueva", "Crear", "Institucion")</li>
                    </ul>
                </li>
                <li>
                    <a href="@Url.Action("Catalogos", "Catalogo")">
                        <i class="fa fa-list-ul"></i>
                        <span>Catálogos</span>
                    </a>
                </li>
            </ul>
        </div>
        <div id="content" class="content">
            <div class="panel-body">
                @RenderBody()
            </div>
        </div>
        <a href="javascript:;" class="btn btn-icon btn-circle btn-success btn-scroll-to-top fade" data-click="scroll-top"><i class="fa fa-angle-up"></i></a>
        <div class="footer-fluid">
            <div class="footer-right">
                    <p>Powered by:</p>
            </div>
            <div class="footer-rightImg">
                <img src="~/img/th.png" style="height:70%" alt="" />
            </div>
        </div>
    </div>
    @*<div class="container body-content">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
            </footer>
        </div>*@

    <!-- init page-->
    
    
    <!--[if lt IE 9]>
        <script src="assets/crossbrowserjs/html5shiv.js"></script>
